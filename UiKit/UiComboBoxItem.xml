<Element
  minWidth="100%"
  height="34dp"
  alignItemsHor="Start"
  borderRadius="4"
  marginTop="2dp"
  marginBottom="2dp"
  paddingRight="14dp"
  overflow="Hidden">
  <include path="UiColor.h" />

  <Element
      element="mHover"
      positionType="Absolute"
      top="0"
      left="0"
      bottom="0"
      right="0">
  </Element>

  <Element
    element="mSelection"
    width="3dp"
    height="18dp"
    left="2dp"
    borderRadius="0.5"
    backgroundColor="UiColor::Primary">
  </Element>

  <UiText
    element="mTitle"
    marginLeft="14dp">
  </UiText>

</Element>