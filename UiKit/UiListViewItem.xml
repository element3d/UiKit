<UiHoverable
  width="100%"
  height="34dp"
  alignItemsHor="Start"
  borderRadius="3"
  marginBottom="2dp"
  overflow="Hidden">
  <include path="UiColor.h" />

  <Element
    element="mSelection"
    width="3dp"
    height="16dp"
    left="2dp"
    borderRadius="0.5"
    backgroundColor="UiColor::Primary">
  </Element>

  <UiText
    element="mTitle"
    fontSize="14dp"
    marginLeft="12dp"
    textColor="glm::vec4(50, 50, 50, 255)">
  </UiText>

</UiHoverable>