<Element
    backgroundColor="glm::vec4(248, 248, 248, 255)"
    borderRadius="4"
    borderSize="1dp"
    borderColor="glm::vec4(220, 220, 220, 255)"
    height="31dp">
    <Input
        width="150dp"
        height="31dp" 
        textColor="glm::vec4(50, 50, 50, 255)"
        fontFamily="Segoe"
        textColor="glm::vec4(60, 60, 60, 255)"
        fontSize="14"
        paddingLeft="0"
        overflow="Hidden">

        <include path="UiColor.h" />
       
    </Input>
    <Element
        width="30dp"
        height="100%"
        orientation="Vertical">
        <UiIcon
            charcode="E70E"
            width="8dp"
            aspectRatio="1">
        </UiIcon>
         <UiIcon
            charcode="E70D"
            width="8dp"
            aspectRatio="1">
        </UiIcon>
    </Element>
    <Element
        width="100%"
        height="1dp"
        backgroundColor="UiColor::Primary"
        positionType="Absolute"
        bottom="0"
        left="0">
    </Element>
    <Element
        padding="4dp"
        backgroundColor="glm::vec4(245, 245, 245, 255)"
        borderRadius="8"
        borderSize="1dp"
        borderColor="glm::vec4(220, 220, 220, 255)"
        orientation="Vertical">
        <Shadow
            blurSize="8"
            offset="x: 0, y: 2"
            color="glm::vec4(150, 150, 150, 255)"
            opacity="1">
        </Shadow>

        <UiAppBarButton
            width="36dp"
            height="36dp"
            icon="_s(E70E)"
            >
        </UiAppBarButton>
         <UiAppBarButton
            width="36dp"
            height="36dp"
            icon="_s(E70D)"
            >
        </UiAppBarButton>
    </Element>
</Element>