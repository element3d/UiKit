<Element
    width="200dp"
    alignItemsHor="Start">
    <include path="UiColor.h"/>
    <Element
        element="mBG"
        width="100%"
        height="4dp"
        backgroundColor="glm::vec4(150, 150, 150, 255)"
        borderRadius="0.5"
        positionType="Absolute">
    </Element>
    <Element
        element="mValue"
        width="1"
        height="4dp"
        backgroundColor="UiColor::Primary"
        borderRadius="0.5"
        positionType="Absolute">
    </Element>
    <Element
        element="mCircle"
        width="20dp"
        aspectRatio="1"
        borderSize="1dp"
        borderColor="glm::vec4(220, 220, 220, 255)"
        backgroundColor="glm::vec4(255)"
        borderRadius="0.5">

        <Element
            element="mInnerCircle"
            width="10dp"
            aspectRatio="1"
            backgroundColor="UiColor::Primary"
            borderRadius="0.5">
        </Element>

    </Element>
</Element>