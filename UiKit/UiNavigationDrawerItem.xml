<Element
    width="100%"
    orientation="Vertical">
    <include path="UiColor.h" />
    <Element
        element="mHeader"
        marginBottom="4dp"
        width="100%"
        height="34dp"
        borderRadius="3"
        overflow="Hidden"
        alignItemsHor="Start">
        <Element
            element="mHover"
            positionType="Absolute"
            width="100%"
            height="100%">
        </Element>
        <Element
            element="mSelection"
            width="3dp"
            borderRadius="0.5"
            height="16dp"
            backgroundColor="UiColor::Primary"
            positionType="Absolute"
            left="0dp"
            visibility="Gone">
        </Element>
        <Element
            element="mContainer"
            alignItemsHor="Start"
            width="100%"
            height="100%"
            marginLeft="12dp"
            scaling="Shrink">
            <UiIcon
                element="mIcon"
                width="16dp"
                aspectRatio="1">
            </UiIcon>
            <UiText
                element="mTitle"
                marginLeft="20dp"
                text="Title">
            </UiText>
        </Element>
        <UiIcon
            element="mExpandIcon"
            marginLeft="16dp"
            marginRight="16dp"
            width="8dp"
            height="8dp"
            color="glm::vec4(00, 0, 0, 255)"
            charcode="E70D">
        </UiIcon>
    </Element>
    <Element
        element="mChildCont"
        alignItemsVer="End"
        overflow="Hidden"
        orientation="Vertical"
        width="100%">
    </Element>
</Element>
  