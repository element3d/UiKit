<UiComboBoxHeader
    width="100%"
    height="34dp"
    alignItemsHor="SpaceBetween"
    backgroundColor="glm::vec4(252, 252, 252, 255)"
    borderRadius="6"
    borderSize="1dp"
    borderColor="glm::vec4(200, 200, 200, 255)">
    <include path="UiColor.h" />

    <Text
        element="mText"
        marginLeft="10dp"
        fontSize="14dp"
        text="Pick A Color"
        font="_s(UiKit/fonts/SegoeUIVF.ttf)"
        textColor="glm::vec4(50, 50, 50, 255)">
    </Text>

    <FontIcon
        element="mIcon"
        marginLeft="14dp"
        marginRight="14dp"
        width="8dp"
        height="8dp"
        color="glm::vec4(0, 0, 0, 255)"
        font="_s(UiKit/icons/Segoe_Fluent_Icons.ttf)"
        charcode="E011">
    </FontIcon>

</UiComboBoxHeader>