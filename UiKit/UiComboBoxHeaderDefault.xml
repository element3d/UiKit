<UiComboBoxHeader
    height="30dp"
    alignItemsHor="SpaceBetween"
    backgroundColor="glm::vec4(252, 252, 252, 255)"
    borderRadius="3"
    borderSize="1dp"
    borderColor="glm::vec4(220, 220, 220, 255)">
    <include path="UiColor.h" />

    <UiText
        element="mText"
        marginLeft="10dp"
        fontSize="14dp"
        text="Pick A Color"
        textColor="glm::vec4(50, 50, 50, 255)">
    </UiText>
    <Element 
        element="mIconCont"   
        width="16dp"
        height="18dp"
        marginLeft="2dp"
        marginRight="2dp"
        borderRadius="4"
        backgroundColor="glm::vec4(60, 60, 67, 0.18 * 255)">
        <UiIcon
            element="mIcon"
            width="8dp"
            aspectRatio="1"
            color="glm::vec4(0, 0, 0, 255)"
            charcode="E70D">
        </UiIcon>
    </Element> 
</UiComboBoxHeader>